<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="report_course">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h2>Courses Report</h2>
                            <p>Course Name: <span><t t-out="o.title"/></span></p>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>